<table>
<tr><th colspan="5" style="text-align:center;"><span style="color:#009900">Standings</span></th></tr>
{% for team_id in leagues[league_id].teams %}
{% set team = leagues[league_id].teams[team_id] %}
<tr><td style="width:16px; text-align:right;">#{{ team.rank }}</td><td style="width:16px; text-align:center;">{{ team.icon }}</td><td><a href="{{ team.url }}">{{ team.name }}</a> ({{ team.username }})</td><td>{{ team.wins }}-{{ team.losses }}</td><td>PF: {{ team.points_for }}</td></tr>
{% endfor %}
</table>
