<table>
<tr><th colspan="4" style="text-align:center;"><span style="color:#009900">Standings - Week {{ leagues[league_id].week }}</span></th></tr>
{% for team_id in leagues[league_id].teams %}
{% set team = leagues[league_id].teams[team_id] %}
<tr><td>#{{ team.rank }}</td><td><a href="{{ team.url }}">{{ team.name }}</a> ({{ team.username }})</td><td>{{ team.wins }}-{{ team.losses }}</td><td>PF: {{ team.points_for }}</td></tr>
{% endfor %}
</table>
